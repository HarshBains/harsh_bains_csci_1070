    "def func(n):\n",
    "    x = 1\n",
    "    while x <= n:\n",
    "        print(x)\n",
    "        x = x + 1\n",
    "        if x % 3 == 0 and x % 5 == 0:\n",
    "            print(x, 'fizzbuzz')\n",
    "        elif x % 3 == 0:\n",
    "            print(x, 'fizz')\n",
    "        elif x % 5 == 0:\n",
    "            print(x, 'buzz')\n",
    "\n",
    "func(15)"
   ]
